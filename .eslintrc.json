{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-undef": "error",
    "no-unused-vars": "error",
    "new-cap": "error",
    "no-prototype-builtins": "off",
    "no-plusplus": "error",
    "no-trailing-spaces": "error",
    "strict": "off",
    "semi": [
      "error",
      "always"
    ],
    "no-unused-expressions": "off",
    "max-len": [
      "error",
      {
        "code": 50
      }
    ]
  },
    "plugins": ["react"],
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}